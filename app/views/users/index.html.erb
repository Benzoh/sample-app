<h1>Listing users</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <!-- @posts.eachで　SELECT "posts".* FROM "posts" クエリが発行される。 -->
    <% @users.each do |user| %>
      <%= debug user.id %>
      <%= debug user.posts %>
    <% end %>
  </tbody>
</table>